  <foxml:datastream CONTROL_GROUP="X" ID="ISLANDORACM" STATE="A" VERSIONABLE="true">
    <foxml:datastreamVersion CREATED="2011-03-24T11:25:23.651Z" ID="ISLANDORACM.0" LABEL="Sub-Collection"  MIMETYPE="application/xml" SIZE="8613">
      <foxml:xmlContent>
<content_model xmlns="http://www.islandora.ca" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="epistemetec:sub_collection" xsi:schemaLocation="http://www.islandora.ca http://localhost/islandoracm.xsd">
  <mimetypes>
    <type>image/jpeg</type>
    <type>image/png</type>
  </mimetypes>
  <ingest_rules>
      <rule>
          <applies_to>image/jpeg</applies_to>
          <applies_to>image/png</applies_to>
          <ingest_methods>
		        <ingest_method class="ImageManipulation" dsid="TN" file="plugins/ImageManipulation.inc" method="createThumbnail" modified_files_ext="jpg" module="fedora_repository">
		          <parameters>
		            <parameter name="width">100</parameter>
		            <parameter name="height">120</parameter>
		          </parameters>
		        </ingest_method>
            <ingest_method class="ImageManipulation" dsid="PRE" file="plugins/ImageManipulation.inc" method="createPreview" modified_files_ext="jpg" module="fedora_repository">
              <parameters>
                <parameter name="width">240</parameter>
                <parameter name="height">300</parameter>
              </parameters>
            </ingest_method>
            <ingest_method class="ImageManipulation" dsid="MEDIUM_SIZE" file="plugins/ImageManipulation.inc" method="manipulateImage" modified_files_ext="jpg" module="fedora_repository">
              <parameters>
                <parameter name="width">240</parameter>
                <parameter name="height">300</parameter>
              </parameters>
            </ingest_method>
		      </ingest_methods>
      </rule>
     </ingest_rules>
  <datastreams>
    <datastream dsid="MEDIUM_SIZE">
      <display_method class="ShowDemoStreamsInFieldSets" file="plugins/ShowDemoStreamsInFieldSets.inc" method="showMediumSize" module="fedora_repository"/>
    </datastream>
    <datastream dsid="OBJ">
      <display_method class="MagShowStreamsInFieldSets" file="plugins/MagShowStreamsInFieldSets.inc" method="showMag" module="islandora_mag"/>
    </datastream>
  </datastreams>
  <ingest_form dsid="MAG" page="2">
    <form_builder_method class="MagFormBuilder" file="plugins/MagFormBuilder.inc" handler="handleXMLForm" method="magBuildForm" module="islandora_mag"/>
    <form_elements>

      <element label="Titolo" name="metadigit][bib][dc:title" required="true" type="textfield" lang="it">
        <description>Un nome dato alla risorsa e con il quale tale risorsa è formalmente conosciuta.</description>
      </element>
      <element label="Title" name="metadigit][bib][dc:title" required="true" type="textfield" lang="en">
        <description>The name given to the resource</description>
      </element>

      <element label="Creator" name="metadigit][bib][dc:creator" type="textfield" lang="en">
        <description>An entity primarily responsible for making the content of the resource such as a person, organization or service.</description>
      </element>
      <element label="Creatore" name="metadigit][bib][dc:creator" type="textfield" lang="it">
        <description>Un'entità (una persona, un'organizzazione o un servizio) che ha la responsabilità principale della produzione del contenuto della risorsa</description>
      </element>

      <element label="Subject" name="metadigit][bib][dc:subject" type="select_taxonomies" lang="en">
        <description>The topic of the resource.</description>
      </element>
      <element label="Soggetto" name="metadigit][bib][dc:subject" type="select_taxonomies" lang="it">
        <description>L’argomento della risorsa.</description>
      </element>

      <element label="Description" name="metadigit][bib][dc:description" required="true" type="textarea" lang="en">
        <description>Examples include an abstract, table of contents, or free-text account of the content of the resource.</description>
      </element>
      <element label="Descrizione" name="metadigit][bib][dc:description" required="true" type="textarea" lang="it">
        <description>Una spiegazione del contenuto della risorsa. Può includere ma non è limitata solo a: un riassunto analitico, un indice, un riferimento al contenuto di una rappresentazione grafica o un testo libero del contenuto.</description>
      </element>

      <element label="Identifier" name="metadigit][bib][dc:identifier" type="hidden" lang="en">
        <description>An unambiguous reference to the resource within a given context.</description>
      </element>
      <element label="Identificatore" name="metadigit][bib][dc:identifier" type="hidden" lang="it">
        <description>Un riferimento univoco alla risorsa nell'ambito di un dato contesto.</description>
      </element>

    </form_elements>
  </ingest_form>
  <edit_metadata_method class="MagFormBuilder" dsid="MAG" file="plugins/MagFormBuilder.inc" handler="magHandleEditMetadataForm" method="magBuildEditMetadataForm" module="islandora_mag"/>
</content_model>
      </foxml:xmlContent>
    </foxml:datastreamVersion>
  </foxml:datastream>
